<div id="body">
    <ul id="contacts"></ul>
</div>
@section scripts{
    <script type="text/javascript">
        $(function()
        {
            $.getJSON('/api/contact', function(contactsJsonPayload)
            {
                $(contactsJsonPayload).each(function(i, item)
                {
                    $('#contacts').append('<li>' + item.Name + '</li>');
                });
            });
        });
    </script>
}